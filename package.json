    "@types/node": "^14.0.23",
    "monaco-editor": "^0.21.2",
    "pad": "^3.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "wabt": "^1.0.19"
  },
  "devDependencies": {
    "@types/jest": "^26.0.5",
    "jest": "^26.1.0",
    "parcel-bundler": "^1.12.4",
    "ts-jest": "^26.1.3"
  }
}
