{
  "name": "demo_compiler",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage=true --runInBand",
    "cleantest": "rm snapshots/*",
    "start": "BUILD_TIME=$(date +%s) parcel serve --port 5002 web/*.html",
